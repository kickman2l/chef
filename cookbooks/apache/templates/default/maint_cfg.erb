<VirtualHost *:80>
    ServerName apache.loc
    DocumentRoot "/var/www/html"
    <Location />
        Order allow,deny
        Allow from all
        Options Indexes
    </Location>
    ProxyPass / http://127.0.0.1:8080/testweb/
    <IfModule mod_rewrite.c>
        RewriteEngine On
        RewriteCond <%= @maintenance_file %> -f
        RewriteCond %{SCRIPT_FILENAME} !/maintenance.html
        RewriteCond %{SCRIPT_FILENAME} !^(.+).(gif|png|jpg|css|js|swf)$
        RewriteRule ^.*$ <%= @maintenance_file %> [L]
    </IfModule>
</VirtualHost>